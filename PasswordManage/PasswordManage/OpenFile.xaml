<Window x:Class="PasswordManage.OpenFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordManage"
        mc:Ignorable="d"
        Title="OpenFile" Height="150" Width="300" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="button" Content="Подтвердить" HorizontalAlignment="Left" Margin="10,89,0,0" VerticalAlignment="Top" Width="75" Click="Confirm"/>
        <Button x:Name="button1" Content="Отмена" HorizontalAlignment="Left" Margin="207,89,0,0" VerticalAlignment="Top" Width="75" Click="Cancel"/>
        <Label x:Name="label" Content="Пароль" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Фаил" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="61,10,0,0" VerticalAlignment="Top" Width="221" Height="26"/>
        <Label x:Name="label2" Content="" HorizontalAlignment="Left" Margin="61,41,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
